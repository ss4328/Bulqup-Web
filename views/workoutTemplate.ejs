<% include partials/header%>     

<!--<h1>You're in  <%= objpassed.workoutName %></h1>-->

<body>
    <!-- Page Content -->
    <div class="container">

        <!-- Portfolio Item Heading -->
        <div class="row">
            <div class="col-lg-12">
                <h1 class="page-header"><%= objpassed.workoutName %>
                    <small><small><%=objpassed.QuickFacts.MusclesWorked%></small></small>
                </h1>
            </div>
        </div>
        <!-- /.row -->

        <!-- Seller Information -->
        <div class="row">

            <div class="col-md-8">
                <object data=<%=objpassed.videoLink%> width="700" height="393"></object>
            </div>

            <div class="col-md-4">
                <div>
                    <h3>Quick-Facts:</h3>

                    <ul>
                        <li>Main muscle worked: <%=objpassed.QuickFacts.MusclesWorked%></li>
                        <li>Type: <%=objpassed.QuickFacts.WorkoutType%></li>
                        <li>Skill Level: <%=objpassed.QuickFacts.SkillLevel%></li>
                        <li>
                            <h5> General Remarks:</h5>
                            <p> <%=objpassed.QuickFacts.GeneralRemark%>
                            </p>
                        </li>
                    </ul>
                </div>
                <hr>


                <div>
                    <h3>Workout:</h3>
                    <div class="btn-group" role="group">
                        <button type="button" class="btn btn-default" onclick="levelchanged(this, <%=objpassed.WorkoutLoad.EasyCount%>)" id = 1>-</button>
                        <button type="button" class="btn btn-default" onclick="levelchanged(this, <%=objpassed.WorkoutLoad.MediumCount%>)" id = 2>&#9733 Medium</button>
                        <button type="button" class="btn btn-default" onclick="levelchanged(this, <%=objpassed.WorkoutLoad.HardCount%>)" id = 3>+</button>
                    </div>

                    <br></br>

                    <ul>
                        <li>Reommended Reps: <span id="repcountID">5</span></li>
                        <li>Recommended Sets: <span id="setCountID">5</span></li>
                    </ul>
                </div>

                <h2>&#9733 <%=objpassed.Rating%>/10</h2>
            </div>
        </div>
        <!-- /.row -->




        <!-- Item Information Row -->
        <div class="row">

            <div class="col-lg-12">
                <h3 class="page-header">Workout Instructions:<i></i></h3>
            </div>

            <div class="col-lg-6 col-xs-12">

                <ol>
                    <li><%=objpassed.workoutInstructions[0]%></li>
                    <li><%=objpassed.workoutInstructions[1]%></li>
                    <li><%=objpassed.workoutInstructions[2]%></li>
                    <li><%=objpassed.workoutInstructions[3]%></li>
                    
                </ol>
                <h3>Tips:</h3>
                <ol>
                    <li><%=objpassed.tips[0]%></li>
                    <li><%=objpassed.tips[1]%></li>
                    <li><%=objpassed.tips[2]%></li>
                    <li><%=objpassed.tips[3]%></li>
                    
                </ol>
            </div>
            <div class="col-lg-6 col-xs-12" style="left: -50px;">
                <img src=<%=objpassed.imagelink%> >
            </div>

        </div>


        <!-- /.row -->

        <!-- Related Projects Row -->
        <div class="row">

            <div class="col-lg-12">
                <h3 class="page-header">Related Workouts:</h3>
            </div>

            <div class="col-sm-3 col-xs-6">
                <a href="#">
                    <img class="img-responsive portfolio-item" src="http://placehold.it/500x300" alt="">
                </a>
            </div>

            <div class="col-sm-3 col-xs-6">
                <a href="#">
                    <img class="img-responsive portfolio-item" src="http://placehold.it/500x300" alt="">
                </a>
            </div>

            <div class="col-sm-3 col-xs-6">
                <a href="#">
                    <img class="img-responsive portfolio-item" src="http://placehold.it/500x300" alt="">
                </a>
            </div>

            <div class="col-sm-3 col-xs-6">
                <a href="#">
                    <img class="img-responsive portfolio-item" src="http://placehold.it/500x300" alt="">
                </a>
            </div>

        </div>
        <!-- /.row -->
        <hr>
    </div>
    <!-- /.container -->

</body>

<br></br>
<hr>

<% include partials/footer%>     
